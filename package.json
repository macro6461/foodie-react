{
    "name": "foodie-react",
    "version": "1.0.8",
    "description": "A package to offer a built-in component for searching and viewing restuarants nearby in your React App.",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "type": "module",
    "directories": {
        "example": "example"
    },
    "peerDependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "build:css": "postcss src/**/*.css --dir dist",
        "build:ts": "npx tsc",
        "build:js": "babel ./dist --out-dir ./dist --extensions '.js'",
        "clean": "rm -rf dist",
        "build": "yarn clean && yarn build:css && yarn tsc && yarn build:js ",
        "test": "react-scripts test"
    },
    "keywords": [
        "react",
        "google",
        "googleapis",
        "google-maps",
        "javascript",
        "react-component",
        "react-library",
        "react-ui",
        "restaurant",
        "restaurants",
        "search",
        "ui"
    ],
    "author": "Matthew Croak",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/macro6461/foodie-react.git",
        "baseUrl": "https://github.com/macro6461/foodie-react"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "files": [
        "README.md",
        "dist",
        "tsconfig.json",
        "CHANGELOG.md",
        "LICENSE",
        "assets"
    ],
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/plugin-transform-runtime": "^7.25.9",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.26.3",
        "@babel/preset-typescript": "^7.26.0",
        "@types/node": "^22.10.1",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.1",
        "autoprefixer": "^10.4.20",
        "babel-plugin-module-extension": "^0.1.3",
        "babel-plugin-transform-assets": "^1.0.2",
        "babel-plugin-transform-rename-import": "^2.3.0",
        "cssnano": "^7.0.6",
        "mini-css-extract-plugin": "^2.9.2",
        "postcss": "^8.4.49",
        "postcss-cli": "^11.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.7.2"
    },
    "proxy": "https://maps.googleapis.com",
    "exports": {
        ".": "./dist/index.js"
    },
    "dependencies": {
        "react-icons": "^5.4.0"
    }
}